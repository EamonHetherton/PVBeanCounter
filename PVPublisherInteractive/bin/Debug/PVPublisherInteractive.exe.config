<?xml version="1.0"?>
<configuration>
   <configSections>
   </configSections>
   <connectionStrings>
      <add name="Service.Properties.Settings.PublishSubscribeConnectionString" connectionString="Data Source=(local);Initial Catalog=PublishSubscribe;Integrated Security=True" providerName="System.Data.SqlClient"/>
   </connectionStrings>
   <system.serviceModel>
      <services>
         <service name="MyPublishService">
            <endpoint address="net.tcp://localhost:8011/MyEventService" binding="netTcpBinding" bindingConfiguration="ReliableTCP" contract="IEnergyEvents"/>
         </service>
         <service name="MySubscriptionService">
            <endpoint address="net.tcp://localhost:8012/MySubscriptionManager" binding="netTcpBinding" bindingConfiguration="ReliableTCP" contract="IMySubscriptionService"/>
            <endpoint address="net.tcp://localhost:8012/PersistentSubscriptionManager" binding="netTcpBinding" bindingConfiguration="ReliableTransactionalTCP" contract="IPersistentSubscriptionService"/>
         </service>
      </services>
      <bindings>
         <netTcpBinding>
            <binding name="ReliableTransactionalTCP" transactionFlow="true">
                <reliableSession enabled="true"/>
            </binding>
            <binding name="ReliableTCP">
                <reliableSession enabled="true"/>
            </binding>
         </netTcpBinding>
      </bindings>
   </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
